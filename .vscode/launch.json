    "version": "0.2.0",
    "configurations": [
    {
        "type": "lldb",
        "request": "attach",
        "name": "Attach",
        "program": "futurecop-mod-injector.exe"
    },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'futurecop-mod-injector'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=futurecop-mod-injector",
                    "--package=futurecop-mod-injector"
                ],
                "filter": {
                    "name": "futurecop-mod-injector",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'futurecop-mod-injector'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=futurecop-mod-injector",
                    "--package=futurecop-mod-injector"
                ],
                "filter": {
                    "name": "futurecop-mod-injector",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}